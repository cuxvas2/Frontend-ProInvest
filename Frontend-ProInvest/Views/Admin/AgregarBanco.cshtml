@model Frontend_ProInvest.Models.BancosViewModel
@{
    Layout = null;
}
<form asp-action="AgregarBanco">
    <div class="form-group">
        <label for="Nombre">Nombre</label>
        <input type="text" class="form-control" id="Nombre" asp-for="Banco" required>
    </div>
    <div class="container-fluid text-end mt-4">
        <button type="submit" class="btn btn-primary">Agregar</button>
    </div>
    
</form>